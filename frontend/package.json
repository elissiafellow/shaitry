    "scripts": {
        "ng": "ng",
        "config": "ts-node set-env.ts",
        "prepare": "node ./prepare.js",
        "start": "npm run config && ng run app:serve --host=localhost --port=8100",
        "start:mock": "npm run config -- --buildMode=mock && ng run app:serve --host=localhost --port=8100",
        "start:staging": "npm run config -- --buildMode=staging && ng run app:serve --host=localhost --port=8100",
        "start:production": "npm run config -- --buildMode=storeRelease && ng run app:serve --host=localhost --port=8100",
        "build": "if [ $CI_APP_ID ]; then echo \"*****Building production release*****\" && ng build --configuration production; else ng build; fi",
        "build:prod": "ng build --configuration production",
        "build:prod:source-map": "ng build --configuration production --source-map",
        "build:ios": "npm run cap-config:ios && npm run config && npm run build && npx cap sync ios && npm run trapeze:ios && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme MoneyFellows -configuration Debug -destination 'generic/platform=iOS' build",
        "build:ios:stage": "npm run cap-config:ios && npm run config -- --buildMode=staging && npm run build && npx cap sync ios && npm run trapeze:ios && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme MoneyFellows -configuration Debug -destination 'generic/platform=iOS' build",
        "build:ios:stage-prod": "npm run cap-config:ios && npm run config -- --buildMode=staging && npm run build && npx cap sync ios && npm run trapeze:ios && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme MoneyFellows -configuration Release -destination 'generic/platform=iOS' build",
        "build:ios:store": "npm run cap-config:ios && npm run config -- --buildMode=storeRelease && npm run build && npx cap sync ios && npm run trapeze:ios && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme MoneyFellows -configuration Release -destination 'generic/platform=iOS' build",
        "build:ios:preprod": "npm run cap-config:ios && npm run config -- --buildMode=preprodRelease && npm run build && npx cap sync ios && npm run trapeze:ios && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme MoneyFellows -configuration Release -destination 'generic/platform=iOS' build",
        "build:android": "npm run cap-config:android && npm run config && npm run build && npx cap sync android && npm run trapeze:android && cd android && ./gradlew assembleDebug",
        "build:android:stage": "npm run cap-config:android && npm run config -- --buildMode=staging && npm run build && npx cap sync android && npm run trapeze:android && cd android && ./gradlew assembleDebug",
        "build:android:stage-prod": "npm run cap-config:android && npm run config -- --buildMode=staging && npm run build && npx cap sync android && npm run trapeze:android && cd android && ./gradlew assembleRelease",
        "build:android:store": "npm run cap-config:android && npm run config -- --buildMode=storeRelease && npm run build && npx cap sync android && npm run trapeze:android && cd android && ./gradlew assembleRelease",
        "build:android:preprod": "npm run cap-config:android && npm run config -- --buildMode=preprodRelease && npm run build && npx cap sync android && npm run trapeze:android && cd android && ./gradlew assembleRelease",
        "build:huawei": "npm run cap-config:huawei && npm run config -- --platform=huawei && npm run build && npx cap sync android && npm run trapeze:huawei && cd android && ./gradlew assembleDebug",
        "build:huawei:stage": "npm run cap-config:huawei && npm run config -- --platform=huawei --buildMode=staging && npm run build && npx cap sync android && npm run trapeze:huawei && cd android && ./gradlew assembleDebug",
        "build:huawei:stage-prod": "npm run cap-config:huawei && npm run config -- --platform=huawei --buildMode=staging && npm run build && npx cap sync android && npm run trapeze:huawei && cd android && ./gradlew assembleRelease",
        "build:huawei:store": "npm run cap-config:huawei && npm run config -- --platform=huawei --buildMode=storeRelease && npm run build && npx cap sync android && npm run trapeze:huawei && cd android && ./gradlew assembleRelease",
        "build:huawei:preprod": "npm run cap-config:huawei && npm run config -- --platform=huawei --buildMode=preprodRelease && npm run build && npx cap sync android && npm run trapeze:huawei && cd android && ./gradlew assembleRelease",
        "test": "ng test --code-coverage",
        "test:silent": "ng test --browsers ChromeHeadless --watch=true --code-coverage",
        "preci:test": "npm run config",
        "ci:test": "ng test --watch=false --code-coverage",
        "ci:increment-code": "node ./scripts/cli/cli.js --increment-code",
        "cli": "node ./scripts/cli/cli.js",
        "ci:download-prod-files": "node ./scripts/cli/helpers/download-aws-files.js",
        "lint": "ng lint",
        "lint:fix": "ng lint --fix",
        "status": "node ./scripts/config/check-versions.js",
        "docs:json": "compodoc -p ./tsconfig.app.json -e json -d .",
        "lighthouse": "node ./lighthouse/start.js",
        "source-map-explorer": "npm run build:prod:source-map && source-map-explorer www/**/*.js",
        "serve:production": "http-server www -p 3333",
        "bundle:wizard": "npx bundle-wizard 127.0.0.1:3333/landing",
        "storybook": "ng run app:storybook",
        "build-storybook": "ng run app:build-storybook",
        "sonar": "sonar-scanner",
        "generate-localization-dictionary": "node ./scripts/cli/generate-localization-dictionary.js",
        "format-localization-dictionary": "prettier --write src/app/core/translations/**",
        "translate": "npm run generate-localization-dictionary && npm run format-localization-dictionary",
        "minify-svgs": "svgo -rf src/assets/images",
        "map:icons": "node ./scripts/cli/icons-mapping.js",
        "cap-config:ios": "node ./scripts/config/set-capacitor-config.js --platform=ios ",
        "cap-config:android": "node ./scripts/config/set-capacitor-config.js --platform=android ",
        "cap-config:huawei": "node ./scripts/config/set-capacitor-config.js --platform=huawei ",
        "sync:ios": "npm run cap-config:ios && npm run build && npx cap sync ios && npm run trapeze:ios",
        "sync:ios:stage": "npm run cap-config:ios && npm run config -- --buildMode=staging && npm run build && npx cap sync ios && npm run trapeze:ios",
        "sync-only:ios": "npm run cap-config:ios  && npx cap sync ios && npm run trapeze:ios",
        "sync-only:android": "npm run cap-config:android && npx cap sync android && npm run trapeze:android",
        "sync-only:huawei": "npm run cap-config:huawei && npx cap sync android && npm run trapeze:huawei",
        "sync:android": "npm run cap-config:android && npm run build && npx cap sync android && npm run trapeze:android ",
        "sync:huawei": "npm run cap-config:huawei && npm run build && npx cap sync android && npm run trapeze:huawei ",
        "sync:android:stage": "npm run cap-config:android && npm run config -- --buildMode=staging && npm run build && npx cap sync android && npm run trapeze:android",
        "sync:huawei:stage": "npm run cap-config:huawei && npm run config -- --buildMode=staging && npm run build && npx cap sync android && npm run trapeze:huawei",
        "trapeze:android": "node generate-trapeze.js --platform=android && npx trapeze run config.yaml -y",
        "trapeze:huawei": "node generate-trapeze.js --platform=huawei && npx trapeze run config.yaml -y",
        "trapeze:ios": "node generate-trapeze.js --platform=ios && npx trapeze run config.yaml -y"
    },
